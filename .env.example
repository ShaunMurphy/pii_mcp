# Example environment variables for FastMCP SFMC integration
# Copy this file to .env and fill in your secrets (never commit .env to source control)

SFMC_CLIENT_ID=
SFMC_CLIENT_SECRET=
SFMC_SUBDOMAIN=
# Optional: SFMC_MID=
# Enable MLFlow tracing (true/false, default: false)
# MLFlow tracking URI (default: http://localhost:5000)
MLFLOW_TRACKING_URI=http://localhost:5000

# MLFlow experiment name (default: FastMCP Tracing)
MLFLOW_EXPERIMENT=FastMCP Tracing
ENABLE_MLFLOW=false